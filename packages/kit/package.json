{
	"name": "@sveltejs/kit",
	"version": "1.0.0-next.45",
	"type": "module",
	"dependencies": {
		"@sveltejs/vite-plugin-svelte": "workspace:*",
		"@svitejs/vite-plugin-svelte": "^0.10.0",
		"cheap-watch": "^1.0.3",
		"sade": "^1.7.4"
	},
	"devDependencies": {
		"@rollup/plugin-replace": "^2.4.1",
		"@sveltejs/app-utils": "workspace:*",
		"@types/amphtml-validator": "^1.0.1",
		"@types/mime": "^2.0.3",
		"@types/node": "^14.14.33",
		"@types/rimraf": "^3.0.0",
		"@types/sade": "^1.7.2",
		"amphtml-validator": "^1.0.34",
		"devalue": "^2.0.1",
		"eslint": "^7.21.0",
		"kleur": "^4.1.4",
		"node-fetch": "^3.0.0-beta.9",
		"port-authority": "^1.1.2",
		"rimraf": "^3.0.2",
		"rollup": "^2.41.1",
		"sirv": "^1.0.11",
		"svelte": "^3.35.0",
		"tiny-glob": "^0.2.8",
		"typescript": "^4.2.3",
		"uvu": "^0.5.1",
		"vite": "^2.0.5"
	},
	"peerDependencies": {
		"svelte": "^3.32.1",
		"vite": "^2.0.4"
	},
	"bin": {
		"svelte-kit": "svelte-kit.js"
	},
	"files": [
		"assets",
		"dist",
		"client"
	],
	"scripts": {
		"dev": "rm -rf assets/runtime && rollup -cw",
		"build": "rm -rf assets/runtime && rollup -c",
		"lint": "eslint --ignore-path .gitignore \"src/**/*.{ts,mjs,js,svelte}\" && npm run check-format",
		"check": "npx tsc",
		"format": "prettier --write . --config ../../.prettierrc --ignore-path .gitignore",
		"check-format": "prettier --check . --config ../../.prettierrc --ignore-path .gitignore",
		"prepublishOnly": "npm run build",
		"test": "npm run test:unit && npm run test:integration",
		"test:unit": "uvu src \"(spec\\.js|test[\\\\/]index\\.js)\"",
		"test:integration": "uvu test test.js"
	},
	"exports": {
		"./ssr": {
			"import": "./dist/ssr.js"
		}
	},
	"types": "types.d.ts"
}
